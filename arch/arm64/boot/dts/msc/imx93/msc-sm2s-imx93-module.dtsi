	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&pinctrl_lpspi3>;
	pinctrl-1 = <&pinctrl_lpspi3>;
	fsl,spi-num-chipselects = <2>;
	cs-gpios =
			<&gpio2 8 GPIO_ACTIVE_LOW>,
			<&gpio2 7 GPIO_ACTIVE_LOW>;
	status = "okay";

	spidev0_0: spi@0 {
		compatible = "rohm,dh2228fv";
		reg = <0>;
		spi-max-frequency = <500000>;
	};

	spidev0_1: spi@1 {
		compatible = "rohm,dh2228fv";
		reg = <1>;
		spi-max-frequency = <500000>;
	};
};

